{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "mn": {
      "title": "Match Name",
      "description": "After Effect's Match Name. Used for expressions.",
      "type": "string"
    },
    "nm": {
      "title": "Name",
      "description": "After Effect's Name. Used for expressions.",
      "type": "string"
    },
    "ty": {
      "title": "Type",
      "description": "Shape content type.",
      "type": "string",
      "const": "st"
    },
    "lc": {
      "title": "Line Cap",
      "description": "Stroke Line Cap",
      "oneOf": [
        {
          "$ref": "https://fancy-lottie.github.io/lottie-schema/helpers/lineCap.json"
        }
      ],
      "type": "number"
    },
    "lj": {
      "title": "Line Join",
      "description": "Stroke Line Join",
      "oneOf": [
        {
          "$ref": "https://fancy-lottie.github.io/lottie-schema/helpers/lineJoin.json"
        }
      ],
      "type": "number"
    },
    "ml": {
      "title": "Miter Limit",
      "description": "Stroke Miter Limit. Only if Line Join is set to Miter.",
      "type": "number"
    },
    "o": {
      "title": "Opacity",
      "description": "Stroke Opacity",
      "oneOf": [
        {
          "$ref": "https://fancy-lottie.github.io/lottie-schema/properties/Value.json"
        },
        {
          "$ref": "https://fancy-lottie.github.io/lottie-schema/properties/valueKeyframed.json"
        }
      ],
      "type": "object"
    },
    "w": {
      "title": "Width",
      "description": "Stroke Width",
      "oneOf": [
        {
          "$ref": "https://fancy-lottie.github.io/lottie-schema/properties/Value.json"
        },
        {
          "$ref": "https://fancy-lottie.github.io/lottie-schema/properties/valueKeyframed.json"
        }
      ],
      "type": "object"
    },
    "c": {
      "title": "Color",
      "description": "Stroke Color",
      "oneOf": [
        {
          "$ref": "https://fancy-lottie.github.io/lottie-schema/properties/multiDimensional.json"
        },
        {
          "$ref": "https://fancy-lottie.github.io/lottie-schema/properties/multiDimensionalKeyframed.json"
        }
      ],
      "type": "object"
    }
  }
}
