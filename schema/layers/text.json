{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "ty": {
      "title": "Type",
      "description": "图层类型：文字。",
      "type": "number",
      "value": 5
    },
    "ks": {
      "title": "Transform",
      "description": "转换属性",
      "oneOf": [{
        "$ref": "https://lottie-lint.github.io/lottie-schema/helpers/transform.json"
      }],
      "type": "object"
    },
    "ao": {
      "title": "Auto-Orient",
      "description": "沿路径AE属性自动定向。",
      "oneOf": [{
        "$ref": "https://lottie-lint.github.io/lottie-schema/helpers/boolean.json"
      }],
      "type": "number",
      "default": 0
    },
    "bm": {
      "title": "Blend Mode",
      "description": "混合模式",
      "oneOf": [{
        "$ref": "https://lottie-lint.github.io/lottie-schema/helpers/blendMode.json"
      }],
      "type": "number",
      "default": 0
    },
    "ddd": {
      "title": "3d Layer",
      "description": "3d图层标志",
      "oneOf": [{
        "$ref": "https://lottie-lint.github.io/lottie-schema/helpers/boolean.json"
      }],
      "type": "number",
      "default": 0
    },
    "ind": {
      "title": "Index",
      "description": "AE中的层索引。用于父母和表达式。",
      "type": "number"
    },
    "cl": {
      "title": "Class",
      "description": "在SVG / HTML渲染器上用作html类的解析图层名称",
      "type": "string"
    },
    "ln": {
      "title": "layer HTML ID",
      "description": "在SVG / HTML渲染器上用作html id的解析图层名称",
      "type": "string"
    },
    "ip": {
      "title": "In Point",
      "description": "初始帧",
      "type": "number"
    },
    "op": {
      "title": "Out Point",
      "description": "结束帧",
      "type": "number"
    },
    "st": {
      "title": "Start Time",
      "description": "图层的开始时间。设置图层的开始时间。",
      "type": "number"
    },
    "nm": {
      "title": "Name",
      "description": "After Effects图层名称。用于KeyPath。",
      "type": "number"
    },
    "hasMask": {
      "title": "Has Masks",
      "description": "图层具有蒙版时的布尔值。将弃用以支持检查maskProperties。",
      "type": "number"
    },
    "maskProperties": {
      "title": "Mask Properties",
      "description": "面具清单",
      "items": {
        "oneOf": [{
          "$ref": "https://lottie-lint.github.io/lottie-schema/helpers/mask.json"
        }],
        "type": "object"
      },
      "type": "array"
    },
    "ef": {
      "title": "Effects",
      "description": "沿路径AE属性自动定向。",
      "oneOf": [{
        "$ref": "https://lottie-lint.github.io/lottie-schema/helpers/boolean.json"
      }],
      "type": "number"
    },
    "sr": {
      "title": "Stretch",
      "description": "图层时间拉伸",
      "type": "number",
      "default": 1
    },
    "parent": {
      "title": "Parent",
      "description": "图层父级。使用父母的ind。",
      "type": "number"
    },
    "t": {
      "title": "Text Data",
      "description": "文本数据",
      "properties": {
        "a": {
          "title": "Animators",
          "description": "文本动画",
          "items": {
            "properties": {
              "a": {
                "title": "Animated Properties",
                "description": "文本动画动画属性",
                "properties": {
                  "p": {
                    "title": "Position",
                    "description": "文本动画位置",
                    "oneOf": [{
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/multiDimensional.json"
                      },
                      {
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/multiDimensionalKeyframed.json"
                      }
                    ],
                    "type": "object"
                  },
                  "a": {
                    "title": "Anchor Point",
                    "description": "文本动画锚点",
                    "oneOf": [{
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/multiDimensional.json"
                      },
                      {
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/multiDimensionalKeyframed.json"
                      }
                    ],
                    "type": "object"
                  },
                  "s": {
                    "title": "Scale",
                    "description": "文本动画比例",
                    "oneOf": [{
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/multiDimensional.json"
                      },
                      {
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/multiDimensionalKeyframed.json"
                      }
                    ],
                    "type": "object"
                  },
                  "sk": {
                    "title": "Skew",
                    "description": "文本动画歪斜",
                    "oneOf": [{
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/value.json"
                      },
                      {
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/valueKeyframed.json"
                      }
                    ],
                    "type": "object"
                  },
                  "sa": {
                    "title": "Skew Axis",
                    "description": "文本动画Skew Axis",
                    "oneOf": [{
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/value.json"
                      },
                      {
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/valueKeyframed.json"
                      }
                    ],
                    "type": "object"
                  },
                  "r": {
                    "title": "Rotation",
                    "description": "文本动画旋转",
                    "oneOf": [{
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/value.json"
                      },
                      {
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/valueKeyframed.json"
                      }
                    ],
                    "type": "object"
                  },
                  "o": {
                    "title": "Opacity",
                    "description": "文本动画不透明度",
                    "oneOf": [{
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/value.json"
                      },
                      {
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/valueKeyframed.json"
                      }
                    ],
                    "type": "object"
                  },
                  "sw": {
                    "title": "Stroke Width",
                    "description": "文本动画笔画宽度",
                    "oneOf": [{
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/value.json"
                      },
                      {
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/valueKeyframed.json"
                      }
                    ],
                    "type": "object"
                  },
                  "sc": {
                    "title": "Stroke Color",
                    "description": "文本动画笔画颜色",
                    "oneOf": [{
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/multiDimensional.json"
                      },
                      {
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/multiDimensionalKeyframed.json"
                      }
                    ],
                    "type": "object"
                  },
                  "fc": {
                    "title": "Fill Color",
                    "description": "文本动画填充颜色",
                    "oneOf": [{
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/multiDimensional.json"
                      },
                      {
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/multiDimensionalKeyframed.json"
                      }
                    ],
                    "type": "object"
                  },
                  "fh": {
                    "title": "Fill Hue",
                    "description": "文本动画填充色调",
                    "oneOf": [{
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/value.json"
                      },
                      {
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/valueKeyframed.json"
                      }
                    ],
                    "type": "object"
                  },
                  "fs": {
                    "title": "Fill Saturation",
                    "description": "文本动画填充饱和度",
                    "oneOf": [{
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/value.json"
                      },
                      {
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/valueKeyframed.json"
                      }
                    ],
                    "type": "object"
                  },
                  "fb": {
                    "title": "Fill Brightness",
                    "description": "文本动画填充亮度",
                    "oneOf": [{
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/value.json"
                      },
                      {
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/valueKeyframed.json"
                      }
                    ],
                    "type": "object"
                  },
                  "t": {
                    "title": "Tracking",
                    "description": "文本动画跟踪",
                    "oneOf": [{
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/value.json"
                      },
                      {
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/valueKeyframed.json"
                      }
                    ],
                    "type": "object"
                  }
                },
                "type": "object"
              },
              "r": {
                "title": "Range Selecton",
                "description": "动画师范围选择",
                "properties": {
                  "t": {
                    "title": "Type",
                    "description": "选择器类型。可表达或正常。",
                    "type": "number"
                  },
                  "maxa": {
                    "title": "Max Amount",
                    "description": "选择器最大金额",
                    "oneOf": [{
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/value.json"
                      },
                      {
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/valueKeyframed.json"
                      }
                    ],
                    "type": "number"
                  },
                  "mine": {
                    "title": "Min Ease",
                    "description": "级别敏捷",
                    "oneOf": [{
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/value.json"
                      },
                      {
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/valueKeyframed.json"
                      }
                    ],
                    "type": "number"
                  },
                  "maxe": {
                    "title": "Max Ease",
                    "description": "级别最大限度",
                    "oneOf": [{
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/value.json"
                      },
                      {
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/valueKeyframed.json"
                      }
                    ],
                    "type": "number"
                  },
                  "r": {
                    "title": "Randomize",
                    "description": "选择器随机化顺序",
                    "oneOf": [{
                      "$ref": "https://lottie-lint.github.io/lottie-schema/helpers/boolean.json"
                    }],
                    "type": "number"
                  },
                  "s": {
                    "title": "Shape",
                    "description": "选择器形状",
                    "oneOf": [{
                      "$ref": "https://lottie-lint.github.io/lottie-schema/helpers/textShape.json"
                    }],
                    "type": "number"
                  },
                  "bo": {
                    "title": "Based On",
                    "description": "基于的选择器",
                    "oneOf": [{
                      "$ref": "https://lottie-lint.github.io/lottie-schema/helpers/textBased.json"
                    }],
                    "type": "number"
                  },
                  "ru": {
                    "title": "Range Units",
                    "description": "选择器范围单位。百分比或指数。",
                    "type": "number"
                  },
                  "start": {
                    "title": "Start",
                    "description": "选择器启动",
                    "oneOf": [{
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/value.json"
                      },
                      {
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/valueKeyframed.json"
                      }
                    ],
                    "type": "number"
                  },
                  "end": {
                    "title": "End",
                    "description": "选择器结束",
                    "oneOf": [{
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/value.json"
                      },
                      {
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/valueKeyframed.json"
                      }
                    ],
                    "type": "number"
                  },
                  "Offset": {
                    "title": "Offset",
                    "description": "选择器偏移",
                    "oneOf": [{
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/value.json"
                      },
                      {
                        "$ref": "https://lottie-lint.github.io/lottie-schema/properties/valueKeyframed.json"
                      }
                    ],
                    "type": "number"
                  }
                },
                "type": "object"
              }
            },
            "type": "object"
          },
          "type": "array"
        },
        "m": {
          "title": "More Options",
          "description": "文字更多选项",
          "properties": {
            "a": {
              "title": "Anchor Point Grouping",
              "description": "文本锚点分组",
              "oneOf": [{
                "$ref": "https://lottie-lint.github.io/lottie-schema/helpers/textGrouping.json"
              }],
              "type": "number"
            },
            "g": {
              "title": "Grouping Alignment",
              "description": "文本分组对齐",
              "oneOf": [{
                  "$ref": "https://lottie-lint.github.io/lottie-schema/properties/multiDimensional.json"
                },
                {
                  "$ref": "https://lottie-lint.github.io/lottie-schema/properties/multiDimensionalKeyframed.json"
                }
              ],
              "type": "number"
            }
          },
          "type": "object"
        },
        "p": {
          "title": "Text Path",
          "description": "文字路径 类型可能是Object",
          "type": "number"
        },
        "d": {
          "title": "Document",
          "description": "文本文档数据",
          "properties": {
            "k": {
              "title": "Keyframes",
              "description": "文本文档数据关键帧",
              "items": {
                "oneOf": [{
                  "properties": {
                    "t": {
                      "title": "Time",
                      "description": "关键帧时间",
                      "type": "number"
                    },
                    "s": {
                      "title": "Text Properties",
                      "description": "文字属性",
                      "type": "object",
                      "properties": {
                        "f": {
                          "title": "Font",
                          "description": "文字字体",
                          "type": "string"
                        },
                        "fc": {
                          "title": "Font Color",
                          "description": "文字字体颜色",
                          "type": "array"
                        },
                        "j": {
                          "title": "Justificaiton",
                          "description": "文字对齐",
                          "type": "string"
                        },
                        "lh": {
                          "title": "Line Height",
                          "description": "文字线高度",
                          "type": "number"
                        },
                        "ls": {
                          "title": "letter-spacing",
                          "description": "文字间距",
                          "type": "number"
                        },
                        "s": {
                          "title": "Size",
                          "description": "文字字体大小",
                          "type": "number"
                        },
                        "t": {
                          "title": "Text",
                          "description": "文本字符串值",
                          "type": "string"
                        },
                        "tr": {
                          "title": "Tracking",
                          "description": "文字追踪",
                          "type": "number"
                        }
                      }
                    }
                  }
                }],
                "type": "object"
              },
              "type": "array"
            }
          },
          "type": "object"
        }
      },
      "type": "object"
    }
  }
}