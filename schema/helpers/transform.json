{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "a": {
      "title": "Anchor Point",
      "description": "变换锚点",
      "oneOf": [
        {
          "$ref": "https://lottie-lint.github.io/lottie-schema/properties/multiDimensional.json"
        },
        {
          "$ref": "https://lottie-lint.github.io/lottie-schema/properties/multiDimensionalKeyframed.json"
        },
      ],
      "default": {"a":0, "k":[0, 0, 0]},
      "type": "object"
    },
    "p": {
      "title": "Position",
      "description": "变换位置",
      "oneOf": [
        {
          "$ref": "https://lottie-lint.github.io/lottie-schema/properties/multiDimensional.json"
        },
        {
          "$ref": "https://lottie-lint.github.io/lottie-schema/properties/multiDimensionalKeyframed.json"
        }
      ],
      "default": {"a":0, "k":[0, 0, 0]},
      "type": "object"
    },
    "s": {
      "title": "Scale",
      "description": "变换比例",
      "oneOf": [
        {
          "$ref": "https://lottie-lint.github.io/lottie-schema/properties/multiDimensional.json"
        },
        {
          "$ref": "https://lottie-lint.github.io/lottie-schema/properties/multiDimensionalKeyframed.json"
        }
      ],
      "default": {"a":0, "k":[100, 100, 100]},
      "type": "object"
    },
    "r": {
      "title": "Rotation",
      "description": "变换旋转",
      "oneOf": [
        {
          "$ref": "https://lottie-lint.github.io/lottie-schema/properties/value.json"
        },
        {
          "$ref": "https://lottie-lint.github.io/lottie-schema/properties/valueKeyframed.json"
        }
      ],
      "default": {"a":0, "k":0},
      "type": "object"
    },
    "o": {
      "title": "Opacity",
      "description": "变换不透明度",
      "oneOf": [
        {
          "$ref": "https://lottie-lint.github.io/lottie-schema/properties/value.json"
        },
        {
          "$ref": "https://lottie-lint.github.io/lottie-schema/properties/valueKeyframed.json"
        }
      ],
      "default": {"a":0, "k":100},
      "type": "object"
    },
    "px": {
      "title": "Position X",
      "description": "转换位置X.",
      "oneOf": [
        {
          "$ref": "https://lottie-lint.github.io/lottie-schema/properties/value.json"
        },
        {
          "$ref": "https://lottie-lint.github.io/lottie-schema/properties/valueKeyframed.json"
        }
      ],
      "default": {"a":0, "k":0},
      "type": "object"
    },
    "py": {
      "title": "Position Y",
      "description": "转换位置Y.",
      "oneOf": [
        {
          "$ref": "https://lottie-lint.github.io/lottie-schema/properties/value.json"
        },
        {
          "$ref": "https://lottie-lint.github.io/lottie-schema/properties/valueKeyframed.json"
        }
      ],
      "default": {"a":0, "k":0},
      "type": "object"
    },
    "pz": {
      "title": "Position Z",
      "description": "变换位置Z.",
      "oneOf": [
        {
          "$ref": "https://lottie-lint.github.io/lottie-schema/properties/value.json"
        },
        {
          "$ref": "https://lottie-lint.github.io/lottie-schema/properties/valueKeyframed.json"
        }
      ],
      "default": {"a":0, "k":0},
      "type": "object"
    },
    "sk": {
      "title": "Skew",
      "description": "改变歪斜",
      "oneOf": [
        {
          "$ref": "https://lottie-lint.github.io/lottie-schema/properties/value.json"
        },
        {
          "$ref": "https://lottie-lint.github.io/lottie-schema/properties/valueKeyframed.json"
        }
      ],
      "default": {"a":0, "k":0},
      "type": "object"
    },
    "sa": {
      "title": "Skew Axis",
      "description": "变换斜轴",
      "oneOf": [
        {
          "$ref": "https://lottie-lint.github.io/lottie-schema/properties/value.json"
        },
        {
          "$ref": "http://localhost:8888/schema/properties/valueKeyframed.json"
        }
      ],
      "default": {"a":0, "k":0},
      "type": "object"
    }
  }
}
